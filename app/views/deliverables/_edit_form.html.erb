<% form_for @deliverable, :html => { :multipart => true } do |f| %>
  <%= f.error_messages %>

<p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :comments %><br />
    <%= f.text_area :comments %>
  </p>
  <p>
    <%= f.label :deliverable_type %><br />
    <%= f.select(:deliverable_type, Deliverable::DELIVERABLE_TYPES,
              {:prompt   => "Select a deliverable type"})  %>
  </p>
  <p>
    <%= f.label :person_id %><br />
    <%= f.text_field :person_id %>
  </p>
  <p>
    <%= f.label :team_id %><br />
    <%= f.text_field :team_id %>
  </p>
  <p>
    <%= f.label :course_id %><br />
    <%= f.text_field :course_id %>
  </p>
  <p>
    <%= f.label :task_number %><br />
    <%= f.text_field :task_number %>
  </p>

  <p>
    <%= f.label :deliverable, "Upload your file" %>
    <%= f.file_field :deliverable %>
  </p>
  <p>
    <%= f.submit button_name %>
  </p>
<% end %>

  