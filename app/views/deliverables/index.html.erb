<h1>Listing deliverables</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Comments</th>
    <th>Deliverable type</th>
    <th>Person</th>
    <th>Team</th>
    <th>Course</th>
    <th>Task number</th>
    <th>Deliverable file name</th>
    <th>Deliverable content type</th>
    <th>Deliverable file size</th>
    <th>Deliverable updated at</th>
  </tr>

<% @deliverables.each do |deliverable| %>
  <tr>
    <td><%=h deliverable.name %></td>
    <td><%=h deliverable.comments %></td>
    <td><%=h deliverable.deliverable_type %></td>
    <td><%=h deliverable.person_id %></td>
    <td><%=h deliverable.team_id %></td>
    <td><%=h deliverable.course_id %></td>
    <td><%=h deliverable.task_number %></td>
    <td><%=h deliverable.deliverable_file_name %></td>
    <td><%=h deliverable.deliverable_content_type %></td>
    <td><%=h deliverable.deliverable_file_size %></td>
    <td><%=h deliverable.deliverable_updated_at %></td>
    <td><%= link_to 'Show', deliverable %></td>
    <td><%= link_to 'Edit', edit_deliverable_path(deliverable) %></td>
    <td><%= link_to 'Destroy', deliverable, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New deliverable', new_deliverable_path %>